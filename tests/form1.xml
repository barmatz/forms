<?xml version="1.0" encoding="UTF-8"?>
<form id="form1" uri="formHandler.php" encoding="application/x-www-form-urlencoded" method="get">
	<field name="textField" type="text" default="default text"/>
	<field name="passField" type="password" min="5" max="10"/>
	<field name="checkboxField" type="checkbox" checked="true" value="checkboxValue" enabled="false"/>
	<field name="radioField" type="radio" checked="true" value="radioValue"/>
	<field name="fileField" type="file">
		<accept>jpeg</accept>
		<accept>png</accept>
	</field>
	<field name="hiddenField" type="hidden" value="hiddenVlue"/>
	<submit label="Submit form"/>
</form>